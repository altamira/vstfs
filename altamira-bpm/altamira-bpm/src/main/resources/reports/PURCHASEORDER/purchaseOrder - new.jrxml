	<subDataset name="productsDataset" uuid="b0114484-5912-402a-bb48-bbbaf3e417a6">
		<parameter name="PURCHASE_ORDER_ID" class="java.math.BigDecimal">
			<defaultValueExpression><![CDATA[$P{PURCHASE_ORDER_ID}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT CASE WHEN COUNTER > 1 THEN NULL ELSE "DATE" END AS "DATE",
       ID,
       THICKNESS,
       WIDTH,
       LENGTH,
       LAMINATION,
       TREATMENT,
       WEIGHT,
       TAX,
       UNIT_PRICE,
       ITEM_PRICE
FROM
(
 SELECT ROW_NUMBER() OVER (PARTITION BY POI."DATE" ORDER BY POI."DATE") COUNTER,
        POI."DATE",
        M.ID,
        M.THICKNESS,
        M.WIDTH,
        M.LENGTH,
        M.LAMINATION,
        M.TREATMENT,
        (POI.WEIGHT) AS WEIGHT,
        POI.TAX,
        POI.PRICE AS UNIT_PRICE,
        ((POI.WEIGHT)*POI.PRICE) AS ITEM_PRICE
 FROM PURCHASE_ORDER PO,
      PURCHASE_ORDER_ITEM POI,
      PURCHASE_PLANNING_ITEM PPI,
      REQUEST_ITEM RI,
      MATERIAL M
 WHERE PO.ID = POI.PURCHASE_ORDER
 AND PO.ID = $P{PURCHASE_ORDER_ID}
 AND PPI.ID = POI.PLANNING_ITEM
 AND PPI.REQUEST_ITEM = RI.ID
 AND RI.MATERIAL = M.ID
 ORDER BY POI."DATE"
)]]>
		</queryString>
		<field name="DATE" class="java.sql.Timestamp"/>
		<field name="ID" class="java.math.BigDecimal"/>
		<field name="THICKNESS" class="java.math.BigDecimal"/>
		<field name="WIDTH" class="java.math.BigDecimal"/>
		<field name="LENGTH" class="java.math.BigDecimal"/>
		<field name="LAMINATION" class="java.lang.String"/>
		<field name="TREATMENT" class="java.lang.String"/>
		<field name="WEIGHT" class="java.math.BigDecimal"/>
		<field name="TAX" class="java.math.BigDecimal"/>
		<field name="UNIT_PRICE" class="java.math.BigDecimal"/>
		<field name="ITEM_PRICE" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="altamira_logo" class="java.lang.Object"/>
	<parameter name="PURCHASE_ORDER_ID" class="java.math.BigDecimal"/>
	<parameter name="PURCHASE_ORDER_DATE" class="java.util.Date"/>
	<parameter name="USERNAME" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT PESO_TOTAL,
       TOTAL_DOS_PRODUTOS,
       TOTAL_DO_IPI,
       (SELECT COMMENTS FROM PURCHASE_ORDER PO1 WHERE PO1.ID = $P{PURCHASE_ORDER_ID}) AS COMMENTS
FROM
  (SELECT SUM(WEIGHT) AS PESO_TOTAL,
          SUM(ITEM_PRICE) AS TOTAL_DOS_PRODUTOS,
          SUM((ITEM_PRICE * TAX)/100) AS TOTAL_DO_IPI
   FROM
   (SELECT (POI.WEIGHT) AS WEIGHT,
           POI.TAX,
           ((POI.WEIGHT)*POI.PRICE) AS ITEM_PRICE
    FROM PURCHASE_ORDER PO,
         PURCHASE_ORDER_ITEM POI,
         PURCHASE_PLANNING_ITEM PPI,
         REQUEST_ITEM RI,
         MATERIAL M
    WHERE PO.ID = POI.PURCHASE_ORDER
    AND PO.ID = $P{PURCHASE_ORDER_ID}
    AND PPI.ID = POI.PLANNING_ITEM
    AND PPI.REQUEST_ITEM = RI.ID
    AND RI.MATERIAL = M.ID
    ORDER BY POI."DATE")
  )]]>
	</queryString>
